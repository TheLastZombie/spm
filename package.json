{
  "name": "sshpm",
  "description": "The SSH profile manager.",
  "version": "0.2.0",
  "author": "TheLastZombie",
  "bin": {
    "sshpm": "./bin/run"
  },
  "bugs": "https://github.com/TheLastZombie/sshpm/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "open": "^7.1.0",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "standard": "^14.3.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/TheLastZombie/sshpm",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "sshpm",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "TheLastZombie/sshpm",
  "scripts": {
    "dist": "npx oclif-dev pack",
    "lint": "npx standard --fix",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "start": "node ./bin/run",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}

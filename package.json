{
  "name": "sshpm",
  "description": "The SSH profile manager.",
  "version": "0.4.0",
  "author": "TheLastZombie",
  "bin": {
    "sshpm": "./bin/run"
  },
  "bugs": "https://github.com/TheLastZombie/sshpm/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "cli-ux": "^5.5.0",
    "iconv-lite": "^0.6.2",
    "open": "^7.3.0",
    "rage-edit": "^1.2.0",
    "rclone-bin": "^1.51.0",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^11.0.1",
    "rimraf": "^3.0.2",
    "standard": "^15.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://git.io/sshpm",
  "keywords": [
    "config",
    "configuration",
    "connection",
    "kitty",
    "manager",
    "mremoteng",
    "oclif",
    "openssh",
    "profile",
    "putty",
    "rclone",
    "ssh",
    "winscp",
    "zoc"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "sshpm",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "TheLastZombie/sshpm",
  "scripts": {
    "dist": "npx oclif-dev pack",
    "lint": "npx standard --fix",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "start": "node ./bin/run",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
